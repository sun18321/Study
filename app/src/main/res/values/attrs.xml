<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GifView">
        <attr name="gif" format="reference" />
        <attr name="paused" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CustomTheme">
        <attr name="gifViewStyle" format="reference" />
    </declare-styleable>


    <attr name="ratio" format="float" />
    <attr name="card_padding" format="dimension" />
    <attr name="card_margin" format="dimension" />
    <attr name="card_loop" format="boolean" />
    <attr name="card_max_offset" format="dimension" />
    <attr name="card_scale_rate" format="float" />

    <declare-styleable name="ElasticCardView">
        <attr name="ratio" />
    </declare-styleable>

    <declare-styleable name="CardViewPager">
        <attr name="card_padding" />
        <attr name="card_margin" />
        <attr name="card_scale_rate" />
        <attr name="card_max_offset" />
        <attr name="card_loop" />
    </declare-styleable>


    <attr name="icvp_min_page_scale_offset" format="dimension"/>
    <attr name="icvp_center_page_scale_offset" format="dimension"/>
    <attr name="icvp_min_page_scale" format="float"/>
    <attr name="icvp_max_page_scale" format="float"/>
    <attr name="icvp_medium_scaled" format="boolean"/>
    <attr name="icvp_scroll_duration" format="integer"/>
    <attr name="icvp_page_duration" format="integer"/>
    <attr name="icvp_interpolator" format="reference"/>

    <declare-styleable name="HorizontalInfiniteCycleViewPager">
        <attr name="icvp_min_page_scale_offset"/>
        <attr name="icvp_center_page_scale_offset"/>
        <attr name="icvp_min_page_scale"/>
        <attr name="icvp_max_page_scale"/>
        <attr name="icvp_medium_scaled"/>
        <attr name="icvp_scroll_duration"/>
        <attr name="icvp_page_duration"/>
        <attr name="icvp_interpolator"/>
    </declare-styleable>

    <declare-styleable name="VerticalInfiniteCycleViewPager">
        <attr name="icvp_min_page_scale_offset"/>
        <attr name="icvp_center_page_scale_offset"/>
        <attr name="icvp_min_page_scale"/>
        <attr name="icvp_max_page_scale"/>
        <attr name="icvp_medium_scaled"/>
        <attr name="icvp_scroll_duration"/>
        <attr name="icvp_page_duration"/>
        <attr name="icvp_interpolator"/>
    </declare-styleable>

</resources>